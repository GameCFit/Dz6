using System;
class Program
{
    static void Main(string[] args)
    {
        int albumSize = 52;
        int sizeRow = 3;
        int filledRows = albumSize / sizeRow;
        int excess = albumSize % sizeRow;
        Console.WriteLine($"полных рядов : {filledRows}. Лишних картинок : {excess}");
    }
}
